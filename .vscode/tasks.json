{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: gcc build active file",
      "command": "/sbin/gcc",
      "args": [
        "-fdiagnostics-color=always",
        "-Wall",
        "-Wextra",
        "-std=c11",
        "-I${workspaceFolder}/include",
        "-g",
        "${workspaceFolder}/src/**.c",
        "-o",
        "${workspaceFolder}/main.out"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}
